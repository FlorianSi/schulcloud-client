<li class="card group-card">
    <a href="/courses/{{ courseId._id }}/groups/{{ _id }}">
        <h2 class="card-title h2">{{{ name }}}</h2>
        <div class="card-tags">
            {{#each userIds }}
            <span class="tag tag-default">{{ this.firstName }} {{ this.lastName }}</span>
            {{/each}}
        </div>
    </a>
</li>