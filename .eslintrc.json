{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "globals": {
    "_": true,
    "$": true,
    "after": true,
    "before": true,
    "describe": true,
    "it": true,
    "populateModalForm": true,
    "showAJAXError": true,
    "getQueryParameterByName": true,
    "updateQueryStringParameter": true,
    "populateModal": true,
    "printPart": true,
    "io": true
  },
  "extends": "eslint:recommended",
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2017,
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "experimentalDecorators": true,
      "jsx": true
    },
    "sourceType": "module"
  },
  "plugins": [
    "react",
    "class-property"
  ],
  "rules": {
    "linebreak-style": [
      2,
      "unix"
    ],
    "semi": [
      2,
      "always"
    ],
    "no-unused-vars": [
      0
    ],
    "unicode-bom": ["error", "never"]
  }
}